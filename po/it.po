#: ../bin/calise:176
#, c-format
msgid "Using \"%s\" profile"
msgstr "In uso il profilo \"%s\""

#: ../bin/calise:177
msgid "Camera"
msgstr "Videocamera"

#: ../bin/calise:178
msgid "Sysfs backlight path"
msgstr "Percorso sysfs"

#: ../bin/calise:179
msgid "Backlight steps"
msgstr "Passi di retroilluminazione"

#: ../bin/calise:180
msgid "Delta and offset"
msgstr "Delta e offset"

#: ../bin/calise:181
